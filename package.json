{
    "name": "vue-chat-app",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "concurrently -n server,app \"npm run dev:server\" \"wait-on http://localhost:3000 && npm run dev:client\"",
        "dev:server": "cross-env NODE_ENV=development nodemon server/app.js",
        "dev:client": "vue-cli-service serve",
        "start": "node server/app.js",
        "build:client": "vue-cli-service build",
        "lint": "vue-cli-service lint",
        "heroku-postbuild": "npm run build:client"
    },
    "dependencies": {
        "@babel/plugin-proposal-optional-chaining": "^7.6.0",
        "@babel/register": "^7.6.2",
        "compression": "^1.7.4",
        "core-js": "^3.1.2",
        "dotenv": "^8.2.0",
        "enforce": "^0.1.7",
        "express": "^4.17.1",
        "express-force-https": "^1.0.0",
        "express-sslify": "^1.2.0",
        "helmet": "^3.21.1",
        "nodemon": "^1.19.4",
        "socket.io": "^2.4.0",
        "socket.io-client": "^2.3.0",
        "vue": "^2.6.10",
        "vue-fragment": "^1.5.1",
        "vue-router": "^3.0.6",
        "vuex": "^3.0.1"
    },
    "devDependencies": {
        "@vue/cli-plugin-babel": "^4.0.0",
        "@vue/cli-plugin-eslint": "^4.0.0",
        "@vue/cli-plugin-router": "^4.0.0",
        "@vue/cli-plugin-vuex": "^4.0.0",
        "@vue/cli-service": "^4.0.0",
        "@vue/eslint-config-prettier": "^5.0.0",
        "babel-eslint": "^10.0.1",
        "concurrently": "^5.0.0",
        "cross-env": "^6.0.3",
        "eslint": "^5.16.0",
        "eslint-plugin-prettier": "^3.1.0",
        "eslint-plugin-vue": "^5.0.0",
        "node-sass": "^4.12.0",
        "prettier": "^1.18.2",
        "sass-loader": "^8.0.0",
        "vue-template-compiler": "^2.6.10",
        "wait-on": "^3.3.0"
    }
}
